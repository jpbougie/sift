%li.entry.closed{ :id => "entry_#{entry.id}" }
  .summary
    .box
      %input{ :type => "checkbox" }
    %ul.rating
      - for i in 1..5
        %li.star
          %a{ :href => "#", :class => (entry.rating && i <= entry.rating) ? "active" : "" }= i.to_s
    .meta
      %h4
        %a{ :href => "#entry/#{entry.id}"}= entry.entry
  .details.hidden
    - if entry.stanford
      - for split, domain in entry.stanford["firstlevelsplit"]["result"].zip(entry.stanford["wordnet-firstlevelsplit"]["result"])
        %h3= "Objects: " + split.join(", ")
        %ul.domain
          - for possibility in domain
            %li= possibility
    - else
      %em This entry hasn't been parsed yet